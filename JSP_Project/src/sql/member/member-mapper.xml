<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<comment>SQL</comment>
	
	<entry key="loginMember">
		SELECT *
		FROM MEMBER
		WHERE USER_ID = ?
			AND USER_PWD = ?
			AND STATUS = 'Y'
	</entry>
	
</properties>